{
    "ltex.language": "fr",
    "latex-workshop.synctex.afterBuild.enabled": true,
    "super-figure.vectorFigureEditor": "/usr/bin/inkscape",
    "super-figure.bitmapFigureEditor": "/usr/bin/gimp",
    "latex-workshop.latex.outDir": "%DIR%/.cache",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "copy output",
            "command": "cp",
            "args": [
                "%OUTDIR%/%DOCFILE%.pdf",
                "%DIR%/"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk",
                "copy output"
            ]
        }
    ],
